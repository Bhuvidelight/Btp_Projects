{
  "_version": "1.76.0",
  "sap.app": {
    "id": "orders",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.20.0",
      "toolsId": "9d58c70d-0699-4740-b065-e480647f1bac"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/my-service1/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.143.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "orders.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "OrdersList",
          "target": "OrdersList"
        },
        {
          "pattern": "Orders({key}):?query:",
          "name": "OrdersObjectPage",
          "target": "OrdersObjectPage"
        },
        {
          "pattern": "Orders({key})/OrderstoIssues({key2}):?query:",
          "name": "IssuesObjectPage",
          "target": "IssuesObjectPage"
        }
      ],
      "targets": {
        "OrdersList": {
          "type": "Component",
          "id": "OrdersList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/Orders",
              "variantManagement": "Page",
              "navigation": {
                "Orders": {
                  "detail": {
                    "route": "OrdersObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable"
                  },
                  "columns": {
                    "DataField::CustomerID": {
                      "width": "10rem"
                    }
                  }
                }
              }
            }
          }
        },
        "OrdersObjectPage": {
          "type": "Component",
          "id": "OrdersObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/Orders",
              "navigation": {
                "OrderstoIssues": {
                  "detail": {
                    "route": "IssuesObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "OrderstoIssues/@com.sap.vocabularies.UI.v1.LineItem#Issues": {
                  "columns": {
                    "DataField::IssueID": {
                      "width": "10rem"
                    },
                    "DataField::issueType_code": {
                      "width": "10rem"
                    },
                    "DataField::OrderID": {
                      "width": "10rem"
                    }
                  }
                }
              },
              "content": {
                "footer": {
                  "actions": {
                    "000": {
                      "press": "orders.ext.controller.Bpa_Approval.onClickApproval",
                      "visible": "{= !${ui>/isEditable}}",
                      "enabled": true,
                      "text": "Send for approval"
                    }
                  }
                },
                "body": {
                  "sections": {
                    "ApprovalHistory": {
                      "template": "orders.ext.fragment.ApprovalHistory",
                      "position": {
                        "placement": "After",
                        "anchor": "Issues"
                      },
                      "title": "Approval History"
                    }
                  }
                }
              }
            }
          }
        },
        "IssuesObjectPage": {
          "type": "Component",
          "id": "IssuesObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/Orders/OrderstoIssues",
              "content": {
                "body": {
                  "sections": {
                    "Comments": {
                      "template": "orders.ext.fragment.Comments",
                      "position": {
                        "placement": "After",
                        "anchor": "IssuesInfo"
                      },
                      "title": "Comments section"
                    },
                    "Attachment": {
                      "template": "orders.ext.fragment.Attachment",
                      "position": {
                        "placement": "After",
                        "anchor": "Comments"
                      },
                      "title": "Attachment"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "extends": {
      "extensions": {
        "sap.ui.controllerExtensions": {
          "sap.fe.templates.ObjectPage.ObjectPageController#orders::IssuesObjectPage": {
            "controllerName": "orders.ext.controller.Apply"
          },
          "sap.fe.templates.ObjectPage.ObjectPageController#orders::OrdersObjectPage": {
            "controllerName": "orders.ext.controller.Order"
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
