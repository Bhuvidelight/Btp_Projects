{
  "_version": "1.76.0",
  "sap.app": {
    "id": "approver",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.20.0",
      "toolsId": "ebaca299-9fc9-4e5d-8e60-5ef8dc57b516"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/my-service2/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.143.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "approver.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "IssuesList",
          "target": "IssuesList"
        },
        {
          "pattern": "Issues({key}):?query:",
          "name": "IssuesObjectPage",
          "target": "IssuesObjectPage"
        }
      ],
      "targets": {
        "IssuesList": {
          "type": "Component",
          "id": "IssuesList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/Issues",
              "variantManagement": "Page",
              "navigation": {
                "Issues": {
                  "detail": {
                    "route": "IssuesObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable"
                  }
                }
              }
            }
          }
        },
        "IssuesObjectPage": {
          "type": "Component",
          "id": "IssuesObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/Issues",
              "navigation": {},
              "controlConfiguration": {
                "IssuestoComments/@com.sap.vocabularies.UI.v1.LineItem#Approvercomments": {
                  "tableSettings": {
                    "type": "ResponsiveTable",
                    "selectionMode": "Single"
                  }
                }
              },
              "content": {
                "body": {
                  "sections": {
                    "ApproverComments": {
                      "template": "approver.ext.fragment.ApproverComments",
                      "position": {
                        "placement": "After",
                        "anchor": "GeneratedFacet1"
                      },
                      "title": "{i18n>comments}"
                    },
                    "Attachment": {
                      "template": "approver.ext.fragment.Attachment",
                      "position": {
                        "placement": "After",
                        "anchor": "ApproverComments"
                      },
                      "title": "Attachments"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "extends": {
      "extensions": {
        "sap.ui.controllerExtensions": {}
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
